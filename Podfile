# platform :ios, '9.0'
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'

target 'SurfingGood' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'

  # Pods for SurfingGood

  target 'SurfingGoodTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SurfingGoodUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
